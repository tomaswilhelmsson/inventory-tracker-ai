{
  "name": "inventory-tracker",
  "version": "1.0.0",
  "description": "Full-stack FIFO inventory tracking system",
  "main": "backend/dist/server.js",
  "scripts": {
    "dev:backend": "TS_NODE_TRANSPILE_ONLY=true nodemon --exec ts-node backend/src/server.ts",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start:backend": "node backend/dist/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test": "jest",
    "test:backend": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/services",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "clear:data": "ts-node backend/scripts/clear-data.ts",
    "import:legacy": "ts-node backend/scripts/import-legacy-data.ts",
    "verify:import": "ts-node backend/scripts/verify-import.ts"
  },
  "keywords": [
    "inventory",
    "fifo",
    "tracking",
    "warehouse"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.1",
    "@types/pdfkit": "^0.17.4",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
